    "*!(d).[tj]s": "eslint"
  },
  "main": "./lib/index.js",
  "name": "sequelize",
  "peerDependenciesMeta": {
    "pg": {
      "optional": true
    },
    "pg-hstore": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "ibm_db": {
      "optional": true
    },
    "snowflake-sdk": {
      "optional": true
    },
    "mariadb": {
      "optional": true
    },
    "sqlite3": {
      "optional": true
    },
    "tedious": {
      "optional": true
    }
  },
  "publishConfig": {
    "tag": "latest"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "semantic-release-fail-on-major-bump",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ],
    "branches": [
      "v6",
      {
        "name": "v6-beta",
        "prerelease": "beta"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sequelize/sequelize.git"
